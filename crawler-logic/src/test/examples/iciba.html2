	});
	if($(window).width() < 1380) {
		$backTop.removeClass('w1380').addClass('w1000');
	}
	$(window).resize(function(){
		if($(window).width() > 1380) {
			$backTop.removeClass('w1000').addClass('w1380');
		}else {
			$backTop.removeClass('w1380').addClass('w1000');
		}
	});
	//模块展开和隐藏
	var $info_article_secs = $('.info-article').slice(1);
	$info_article_secs.each(function(index, element){
		var show_icon = $(element).find('.info-display');
		var wenzi = $(element).find('.text-display');
		var icon = $(element).find('.ico-display');
		var article = $(element).find('.article');
		show_icon.click(function(){
			if (!show_icon.hasClass('show')) {
				show_icon.addClass('show');
				article.hide();
				wenzi.text("展开");
				icon.addClass('selected');
			}else {
				show_icon.removeClass('show');
				article.show();
				wenzi.text("收起");
				icon.removeClass('selected');
			}
		});
	});
	//今日receivedtab切换
	var $hotwords_tab_lis = $('.info-hotwords .article-list li');
	var $hotwords_tab_secs = $('.info-hotwords .hotwords-section');
	$hotwords_tab_lis.each(function(index, element){
		$(element).click(function() {
			$hotwords_tab_lis.removeClass('current');
			$(this).addClass('current');
			$hotwords_tab_secs.hide();
			$($hotwords_tab_secs[index]).show();
		});
	});
  //取词划译
	var $getwords = $('.nav-list .getwords');
	var $getwords_ico = $('.nav-list .getwords i');
	$getwords.click(function() {
		if (!$getwords_ico.hasClass('show')) {
				$getwords_ico.addClass('show');
			}else {
				$getwords_ico.removeClass('show');
			}
	});
	//牛津词典新提示
	if(window.localStorage.getItem('point') == 1){
		$('.oxford_i').css("display","none");
	}else{
		$('.oxford_i').css("display","inline-block");
	}
	$('.oxford').click(function(){
		window.localStorage.setItem('point', '1');
		$('.oxford_i').css("display","none");
	});
	
		
	
	
	
	
}(window, jQuery));
</script>

