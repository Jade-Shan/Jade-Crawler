window.ScbName = [];
(function(window, $){
    $('.scb-create').click(function(){
        $('.scb-mask').show();
        $('#textarea').focus();
    });
    $('.scb-btn').click(function(event){
                window.location.href = "http://my.iciba.com/?returnurl=" + window.location.href;
            });
    $('.scb-con').click(function(event){
        event.stopPropagation();
    });
    $(document).click(function(event){
        $('.scb-btn .scb-icon').removeClass('active');
        $('.scb-con').slideUp();
    });
    $('.scb-list').delegate("span", "click", function(){
        var bookname = $(this).text();
        var bookid   = $(this).attr("data-bookid")?$(this).attr("data-bookid"):0;
        setWord(bookname, bookid);
    });
    $('.scb-entry a.go-scb').click(function(){
        syncScb();
        window.open("http://scb.iciba.com");
        return false;
    });
    $('.scb-list').delegate('li', 'click', function(){
        $(this).find('i.check').css({'opacity': '1'}).animate({opacity: 0}, 1500);
    });
    //获取线上生词本数据
    function getScb()
    {
        var uid = "0";
        $.ajax({
            url:"./ajax/notebooklist/" + $('#search_input').val(),
            type:"post",
            data:"&u="+uid,
            dataType:"json",
            success:function(res){
                setScb(res);
                setVersion(res);
            }
        }); 
    }
    //将线上生词本和localstorage生词本名生成html插入到生词本控件中
    function setScb(data){
        var scb_list = $(".scb-list");
        var content = "";
        if (data.status != 0) {
            for(var i = 0, l = data.books.length; i < l; i++){
                content += "<li class='color-" + i % 7 + "'>" + "<i class='circle'></i><span class='name' data-bookid='"+data.books[i].book_id+"'>" + data.books[i].book_name + "</span>" + "<i class='check'>" + "</i>" + "</li>";
                //将线上生词本名push到全局变量ScbName
                ScbName.push(data.books[i].book_name);
            }
        }
        if(window.localStorage.getItem("db_scbLib_0")){
            var LSScbObj = JSON.parse(window.localStorage.getItem("db_scbLib_0"));
            var bookObj = LSScbObj.data.books;
            bookArr = $.map(bookObj, function(value){ return value; });
            for(var i = 0; i < bookArr.length; i++){
                content += "<li class='color-" + i % 7 + "'>" + "<i class='circle'></i><span class='name' data-bookid='0'>" + bookArr[i].n + "</span>" + "<i class='check'>" + "</i>" + "</li>";
            }
        }
        scb_list.html(content);
    }
    //设置生词本版本号
    function setVersion(data){
        window.localStorage.setItem("ScbClientVersion", data.clientVersion?data.clientVersion:0);
    }
    function syncScb()
    {
        var uid = "0";
        var LSScbObj = JSON.parse(window.localStorage.getItem("db_scbLib_0"));
        var bookObj = LSScbObj.data.books;
        var wordObj = LSScbObj.data.words;
        var newScbBook = $.map(bookObj, function(value){ return value; });
        var newScbWord = $.map(wordObj, function(value){ return value; });
        var version = window.localStorage.getItem("ScbClientVersion") ? window.localStorage.getItem("ScbClientVersion") : 0;
        var jsonBook = JSON.stringify(newScbBook);
        var jsonWord = JSON.stringify(newScbWord);
        if (newScbBook.length < 1 && newScbWord.length < 1) {
            return false;
        }
        $.ajax({
            url:"./ajax/notebook/" + version,
            type:"post",
            data:"b="+jsonBook+"&w="+jsonWord+"&u="+uid,
            dataType:"json",
            success:function(res){
                if (res[0].errno == 0) {
                    window.lib.deleteRows("books", function(row){return true;});
                    window.lib.deleteRows("words", function(row){return true;});
                    window.lib.commit();
                    window.localStorage.setItem("ScbClientVersion", res[0].clientVersion)
                }
            }
        });
    }
    function setWord(bookname, bookid)
    {
        var word = "big";
        window.lib.insert("words", {w: word, p: bookname, i: bookid});
        window.lib.commit();
    }
    window.setInterval(syncScb, 60000);
})(window, jQuery);
</script>
    
