<script type='text/javascript'>
$(document).ready(function(){
    window.lib = new localStorageDB('scbLib_0', localStorage);
    (function(window, $){
        //获取全局变量中的线上生词本名数组
        var ScbName = window.ScbName;
        //创建数据库
        //初始化生词本table, 生词table
        if(lib.isNew()){
            lib.createTable("books", ["n"]);
            lib.createTable("words", ["w", "p", "i"]);
            lib.commit();
        }
        $('#textarea').keyup(function(event){
            $('.tip1').hide();
            $('.tip2').hide();
            $('.tip3').hide();
            $('.tip4').hide();
            event.stopPropagation();
        });
        function checkDiffName(value){
            //检测从线上获取到的生词本名称是否有重名
            for(var i = 0; i < ScbName.length; i++){
                if(ScbName[i] == value){
                    $('.tip1').show();
                    return false;
                }
            }
            //检测localstorage里面是否存在生词本，如果存在检测本地生词本名称是否有重名
            if(window.localStorage.getItem("db_scbLib_0")){
                var LSScbObj = JSON.parse(window.localStorage.getItem("db_scbLib_0"));
                var bookObj = LSScbObj.data.books;
                bookArr = $.map(bookObj, function(value){ return value; });
                //console.log(bookArr);
                for(var i = 0; i < bookArr.length; i++){
                    if(bookArr[i].n == value){
                        $('.tip1').show();
                        return false;
                    }
                }
            }
            return true;
        }
        //检测生词本命名是否合法，名称应只包含字母数字和汉字
        function checkValidName(value){
            if (value == "") {
                $('.tip4').show();
                return false;
            } else if(!(/^[A-Za-z0-9\u4e00-\u9fa5]+$/).test(value)){
                $('.tip2').show();
                return false;
            } else if (value.length > 24) {
                $('.tip3').show();
                return false;
            }else {
                return true;
            }
        }
        $('#scb_cancel').click(function(){
            $('.scb-mask').hide();
        });
        $('.scb-mask').click(function(event){
            event.stopPropagation();
        });
        //如果新创建的生词本名无重名并合法，将生词本名记录插入到localstorage生词本table，并加入页面生词本控件中
        (function(){
            var i = 0;
            $('#scb_create').click(function(){
                var value = $('#textarea').val();
                var diff = checkDiffName(value);
                var valid = checkValidName(value);   
                if(diff && valid){
                    //console.log("The name " + value +" is ok");
                    lib.insert("books", {n: value});
                    lib.commit();
                    $('.scb-list').prepend("<li class='color-" + i % 7 + "'>" + "<i class='circle'></i><span class='name'>" + value + "</span>" + "<i class='check'></i>" + "</li>");
                    i++;
                    box.update();
                    $('#textarea').val('');
                    $('.scb-mask').hide();
                }
            });
        })();
        
    })(window, jQuery);
})



</script>
